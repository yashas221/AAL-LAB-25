#include <stdio.h>
#include <stdlib.h>
#include <time.h>

#define MAX_SIZE 1000

int main() {
    int key, n, count = 0, i;
    clock_t start_time, end_time;
    double time_taken;

    printf("Enter the number of elements (up to %d): ", MAX_SIZE);
    scanf("%d", &n);

    if (n > MAX_SIZE) {
        printf("Number of elements exceeds the maximum size of %d\n", MAX_SIZE);
        return 1;
    }

    int a[MAX_SIZE];

    srand(time(0));
    for (i = 0; i < n; i++) {
        a[i] = rand() % 100;
    }

    printf("Array elements:\n");
    for (i = 0; i < n; i++) {
        printf("%d ", a[i]);
    }
    printf("\n");

    key = rand() % 100;
    printf("Random key to search for: %d\n", key);

    start_time = clock();
    for (i = 0; i < n; i++) {
        if (a[i] == key) {
            count++;
        }
    }
    end_time = clock();

    time_taken = ((double)(end_time - start_time)) / CLOCKS_PER_SEC;

    if (count > 0) {
        printf("Element %d is found %d times.\n", key, count);
    } else {
        printf("Element not found\n");
    }

    printf("Time Taken for search in seconds: %f\n", time_taken);

    return 0;
}

Output:
Enter the number of elements (up to 1000): 10
Array elements:
20 12 45 75 52 4 53 1 80 32 
Random key to search for: 33
Element not found
Time Taken for search in seconds: 0.000002
