#include <stdio.h>

void insertionSort(int arr[], int n) {
	int i;
    for (i = 1; i < n; i++) {
        int key = arr[i];
        int j = i - 1;

        
        while (j >= 0 && arr[j] > key) {
            arr[j + 1] = arr[j];
            j--;
        }
        arr[j + 1] = key;
    }
}

void printArray(int arr[], int n) {
	int i;
    for (i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");
}

int main() {
    int length;
    printf("Enter length of elements:");
    scanf("%d",&length);
    printf("Enter %d elements:\n",length);
    int i;
    int arr[length];
    for(i=0;i<length;i++){
    	scanf("%d",&arr[i]);
	}
    int n = sizeof(arr) / sizeof(arr[0]);

    printf("Original array:\n");
    printArray(arr, n);

    insertionSort(arr, n);

    printf("Sorted array:\n");
    printArray(arr, n);

    return 0;
}


OUTPUT:
Enter length of elements:6
Enter 6 elements:
3
78
34
68
45
23
Original array:
3 78 34 68 45 23 
Sorted array:
3 23 34 45 68 78 
